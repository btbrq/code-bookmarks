{
  "name": "Code Bookmarks",
  "description": "Code bookmarks",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [{
    "resources": ["test.json"],
    "matches": [],
    "extension_ids": []
  }],
  "permissions": [
    "storage", "tabs"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z"
      },
      "description": "Opens popup"
    }
  }
}
